#!/usr/bin/gwave -s
!#
; gwave script
(require-n-wavepanels 3)
(set! default-measure1-function 5)
(let ((df (if script-target-datafile
           script-target-datafile
           (find-or-load-wavefile "/home/lucas/Repositories/spice_simu/rawspice.raw"))))
 (wavepanel-add-var-setup df (nth-wavepanel 1) "i(v1)" 3)
 (wavepanel-add-var-setup df (nth-wavepanel 1) "v(out)" 2)
 (wavepanel-add-var-setup df (nth-wavepanel 1) "v(int)" 1)
 (wavepanel-add-var-setup df (nth-wavepanel 1) "v(in)" 0)
)
(x-zoom! 0.0 0.05)
(wtable-set-xlogscale! #f)
(set! default-wavepanel-type 0)
(set-wtable-vcursor! 0 0.009841628959276018)
(let ((wp (nth-wavepanel 0)))
 (set-wavepanel-ylogscale! wp #f)
 (set-wavepanel-type! wp 0)
)
(let ((wp (nth-wavepanel 1)))
 (set-wavepanel-ylogscale! wp #f)
 (set-wavepanel-type! wp 0)
)
(let ((wp (nth-wavepanel 2)))
 (set-wavepanel-ylogscale! wp #f)
 (set-wavepanel-type! wp 0)
)
